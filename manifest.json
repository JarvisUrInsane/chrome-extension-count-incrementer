{
    "manifest_version": 3,
    "name": "Tab Sync",
    "version": "1.0",
    "permissions": ["tabs", "storage"],
    "background": {
      "service_worker": "background.js"
    },
    "host_permissions": ["https://tab-sync-worker.ryanzhou147.workers.dev"],
    "action": {
      "default_popup": "popup.html"
    },
  "icons": {
      "16": "icons/icon16.png",
      "48": "icons/icon48.png",
      "128": "icons/icon128.png"
  }
}